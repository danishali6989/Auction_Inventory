
@model AuctionInventory.Models.Category
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("delete", "MCategory", FormMethod.Post, new { id = "popupForm" }))
{
    @Html.HiddenFor(a => a.iCategoryID)
    @*<div class="form-group">
        <label>Category Name</label>
        <p>@Model.strCategoryName</p>
    </div>*@
    
     <div class="form-group col-xs-5">
        <label>Category Name</label>
        @Html.TextBoxFor(a => a.strCategoryName, new { @class = "form-control  input-group-lg", @placeholder = "Enter Category Name" })
       
    </div>
    
    <div>
        @*<input type="submit" value="Delete" />*@
        <input type="submit" style="margin-left: -350px; margin-top: 100px; " class="btn btn-danger col-md-1" value="Delete">
        
        <input type="button" style="margin-left: -210px;margin-top: 100px;" class="btn btn-warning col-md-1" value="Cancel" onclick="CancelCategoryInfo();" />

    </div>
    
   
}

<script>
    function CancelCategoryInfo() {
        window.location.href = "/MCategory/Index";
    }
</script>
   
