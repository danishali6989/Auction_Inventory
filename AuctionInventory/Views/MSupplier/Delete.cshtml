
@model AuctionInventory.Models.Supplier
@{
    //Layout = null;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Delete Employee</h2>
@using (Html.BeginForm("delete", "MSupplier", FormMethod.Post, new { id = "popupForm" }))
{
    @Html.HiddenFor(a => a.iSupplierID)
    <div class="form-group">
        <label>First Name</label>
        <p>@Model.strFirstName</p>
    </div>

    <div class="form-group">
        <label>Middle Name</label>
        <p>@Model.strMiddleName</p>
    </div>
    <div class="form-group">
        <label>Last Name</label>
        <p>@Model.strLastName</p>
    </div>

    @*<div class="form-group">
        <label>Supplier Category</label>
        <p>@Model.strSupplierCategory</p>
    </div>
    <div class="form-group">
        <label>Supplier ServiceType </label>
        <p>@Model.strSupplierServiceType </p>
    </div>*@

    <div class="form-group">
        <label>Email ID</label>
        <p>@Model.strEmailID</p>
    </div>
    <div class="form-group">
        <label>iPhoneNumber</label>
        <p>@Model.iPhoneNumber</p>
    </div>

    <div class="form-group">
        <label>Address</label>
        <p>@Model.strAddress</p>
    </div>
    <div class="form-group">
        <label>Pincode</label>
        <p>@Model.iPincode</p>
    </div>

    <div class="form-group">
        <label>Currency</label>
        <p>@Model.iCurrency</p>
    </div>
    <div class="form-group">
        <label>Supplier Photo</label>
        <p>@Model.SupplierPhoto</p>
    </div>

    <div class="form-group">
        <label>Supplier Date</label>
        <p>@Model.SupplierDate</p>
    </div>
    <div>
        <input type="submit" value="Delete" />
    </div>
}

